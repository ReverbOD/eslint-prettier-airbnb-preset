{
    "env": {
        "browser": true,
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "airbnb", "prettier", "plugin:node/recommended"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier":"error",
        "no-unusued-vars":"warn",
        "no-console":"off",
        "func-names":"off",
        "no-process-exit": "warn",
        "object-shorthand": "off",
        "class-methods-use-this":"off"
    }
}
